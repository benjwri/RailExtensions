{
  "manifest_version": 3,
 
  "name": "Rail Enthusiast Website Integrations",
  "version": "1.0.0",
  "description": "Provides links between RealTimeTrains, Timetable History, RailChecker and TrackIT!",
 
 
  "content_scripts": [{
    "js": ["RCcontent.js"],
    "matches": ["https://railchecker.app/service/*"]
  },
  {
    "js": ["RTTcontent.js"],
    "matches": ["https://www.realtimetrains.co.uk/service/*/detailed"]
  },
  {
    "js": ["TIcontent.js"],
    "matches": ["https://trackit.uppyjc.co.uk/TrackIT/Forms/Train.aspx*"]
  }]
}